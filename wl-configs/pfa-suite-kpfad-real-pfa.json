{
  "name" : "pfa-suite-kpfad-real-pfa",
  "workdir" : "pfa-exp",
  "post_run_hook" : "resScripts/suite.py",
  "jobs" : [
  {
    "name" : "qsort",
    "base" : "pfa-br.json",
    "linux-config" : "linux-configs/br-kpfad-real-pfa",
    "outputs" : [ "/root/qsort_res.csv", "/root/config.gz" ],
    "command" : "cd root; ./runSuite.sh qsort.cfg"
  },
  {
    "name" : "memblade0",
    "base" : "dummy.json"
  },
  {
    "name" : "genome",
    "base" : "pfa-br.json",
    "linux-config" : "linux-configs/br-kpfad-real-pfa",
    "outputs" : [ "/root/assemble_res.csv", "/root/config.gz" ],
    "command" : "cd root; ./runSuite.sh genome.cfg"
  },
  {
    "name" : "memblade1",
    "base" : "dummy.json"
  },
  {
    "name" : "pagerank",
    "base" : "pfa-br.json",
    "linux-config" : "linux-configs/br-kpfad-real-pfa",
    "outputs" : [ "/root/pagerank_res.csv", "/root/config.gz" ],
    "command" : "cd root; ./runSuite.sh pagerank.cfg"
  },
  {
    "name" : "memblade2",
    "base" : "dummy.json"
  },
  {
    "name" : "test",
    "base" : "pfa-br.json",
    "linux-config" : "linux-configs/br-kpfad-real-pfa",
    "command" : "cd root; ./test.sh"
  },
  {
    "name" : "memblade3",
    "base" : "dummy.json"
  }
  ]
}
